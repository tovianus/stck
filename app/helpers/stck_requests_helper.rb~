module StckRequestsHelper
  def myformclass
    <% frmclass="form-horizontal" %>
    <% frmdivclass="control-group" %>
    <% frmlabelclass="control-label" %>
    <% frminputclass="controls" %>
  end

  def bbm_selects
    select_option=""
    StckRequest::BBM.each do |t|
      select_option+="<option value='#{t}'>t</option>"
    end
    return select_option
  end
end
